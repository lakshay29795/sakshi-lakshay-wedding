{
  "name": "romantic-wedding-website",
  "version": "1.0.0",
  "description": "A romantic wedding website PWA built with Next.js 14, TypeScript, and Tailwind CSS",
  "private": true,
  "keywords": [
    "wedding",
    "pwa",
    "nextjs",
    "typescript",
    "romantic",
    "tailwindcss"
  ],
  "author": "Wedding Couple",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:pwa": "node scripts/test-pwa.js",
    "test:pwa:installation": "playwright test tests/e2e/pwa-installation.spec.ts",
    "test:pwa:offline": "playwright test tests/e2e/pwa-offline.spec.ts",
    "test:pwa:performance": "playwright test tests/e2e/pwa-performance.spec.ts",
    "test:pwa:cross-browser": "playwright test tests/e2e/pwa-cross-browser.spec.ts",
    "test:lighthouse": "lighthouse http://localhost:3000 --only-categories=pwa,performance --output=html --output-path=./test-results/lighthouse-report.html",
    "test:admin": "node scripts/test-admin.js",
    "test:admin:unit": "node scripts/test-admin.js --suite unit",
    "test:admin:integration": "node scripts/test-admin.js --suite integration",
    "test:admin:security": "node scripts/test-admin.js --suite security",
    "test:admin:realtime": "node scripts/test-admin.js --suite realtime",
    "test:admin:e2e": "node scripts/test-admin.js --suite e2e",
    "test:admin:verbose": "node scripts/test-admin.js --verbose",
    "test:e2e:ui": "playwright test --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:bundle": "cross-env ANALYZE=true npm run build && npx webpack-bundle-analyzer .next/static/chunks/*.js",
    "perf:audit": "node scripts/performance-audit.js",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./test-results/lighthouse-performance.html",
    "perf:bundle-size": "npm run build && node scripts/bundle-size-check.js",
    "perf:tree-shake": "node scripts/tree-shaking-analysis.js",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "node scripts/deploy.js --prod",
    "deploy:preview": "vercel --yes",
    "vercel-build": "npm run build",
    "clean": "rm -rf .next out dist"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-email/render": "^1.3.1",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.7",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.22",
    "iron-session": "^8.0.4",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-csrf": "^0.2.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "resend": "^6.1.0",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "validator": "^13.15.15",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.15.3",
    "compression-webpack-plugin": "^11.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lighthouse": "^12.8.2",
    "node-mocks-http": "^1.17.2",
    "playwright-lighthouse": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
