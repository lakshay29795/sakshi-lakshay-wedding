{
  "name": "Sakshi & Lakshay's Wedding",
  "short_name": "S&L Wedding",
  "description": "A romantic wedding website celebrating the love story of Sakshi and Lakshay",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#FFF8F0",
  "theme_color": "#A8B5A0",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "categories": ["lifestyle", "social"],
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "RSVP",
      "short_name": "RSVP",
      "description": "Respond to the wedding invitation",
      "url": "/rsvp",
      "icons": [
        {
          "src": "/icons/rsvp-icon.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Photo Gallery",
      "short_name": "Gallery",
      "description": "View our photo gallery",
      "url": "/gallery",
      "icons": [
        {
          "src": "/icons/gallery-icon.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Our Story",
      "short_name": "Story",
      "description": "Read our love story timeline",
      "url": "/story",
      "icons": [
        {
          "src": "/icons/story-icon.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-home.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Home page on desktop"
    },
    {
      "src": "/screenshots/mobile-home.png",
      "sizes": "375x667",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Home page on mobile"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+wedding",
      "url": "/?handler=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/gallery",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".webp", ".gif"]
      }
    }
  ]
}
